{
  "name": "open-in-github-simple",
  "displayName": "Open in Github Simple",
  "description": "Open the current file in Github",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/ram-nadella/vscode-open-in-github-simple.git"
  },
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "open-in-github-simple.openInGithub",
        "title": "Open in Github"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/proxyquire": "^1.3.31",
    "@types/sinon": "^10.0.16",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "proxyquire": "^2.1.3",
    "sinon": "^15.2.0",
    "typescript": "^5.8.2"
  }
}
